{
  "name": "react-hot-webpack-express-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading and express",
  "scripts": {
    "start": "node ./server/server.js",
    "lint": "eslint src",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack -p --config ./config/webpack.config.production.js",
    "postbuild": "copyfiles index.html dist"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Brandon Jones <brandon.jones1985@gmail.com> (http://github.com/bjonesy)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "copyfiles": "^1.2.0",
    "cross-env": "5.0.1",
    "react-hot-loader": "3.0.0-beta.7",
    "rimraf": "^2.6.0",
    "webpack": "3.3.0",
    "webpack-dev-server": "2.5.1"
  },
  "dependencies": {
    "babel-eslint": "7.2.3",
    "css-loader": "^0.28.0",
    "eslint": "4.2.0",
    "eslint-plugin-react": "7.1.0",
    "node-sass": "^4.5.2",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.18.2"
  }
}
